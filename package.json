{
  "name": "personality-sunburst-chart",
  "version": "2.1.5",
  "description": "Generate Sunburst Chart for Personality Insights Profile",
  "main": "lib/index.js",
  "exportName": "PersonalitySunburstChart",
  "scripts": {
    "prepublish": "npm run compile && npm run build",
    "compile": "node_modules/.bin/webpack",
    "build": "./node_modules/.bin/babel src --presets env --out-dir lib",
    "test": "npm run lint && npm run codecov",
    "lint": "eslint .",
    "autofix": "eslint . --fix",
    "codecov": "istanbul cover mocha test/unit && codecov",
    "pretest-d3": "npm run compile && npm run build && cp -r dist test/d3 && cp -r test/profiles test/d3/profiles",
    "test-d3": "http-server test/d3 -o -c-1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:personality-insights/sunburst-chart.git"
  },
  "engines": {
    "node": ">4.0"
  },
  "author": "IBM Corp.",
  "license": "Apache-2.0",
  "keywords": [
    "personality",
    "insights",
    "sunburst",
    "chart",
    "visualization",
    "ibm",
    "watson"
  ],
  "dependencies": {
    "d3-color": "^1.0.3",
    "d3-hierarchy": "^1.1.5",
    "d3-selection": "^1.3.0",
    "d3-shape": "^1.2.0",
    "object.pick": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "eslint": "^4.18.1",
    "istanbul": "^0.4.3",
    "mocha": "^5.0.1",
    "webpack": "^3.11.0"
  }
}
